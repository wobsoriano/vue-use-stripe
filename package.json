{
  "name": "vue-use-stripe",
  "version": "0.1.1",
  "description": "Thin Vue 3 wrapper for Stripe.js",
  "author": "Fran Dios <me@frandiox.com>",
  "license": "MIT",
  "homepage": "https://github.com/frandiox/vue-use-stripe#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frandiox/vue-use-stripe.git"
  },
  "bugs": {
    "url": "https://github.com/frandiox/vue-use-stripe/issues"
  },
  "keywords": [
    "stripe",
    "elements",
    "vue",
    "vue3",
    "typescript"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "build-fast": "tsup src/index.ts --format cjs,esm",
    "build": "pnpm build-fast --dts-resolve",
    "dev": "vite serve dev",
    "dev:build": "vite build dev",
    "dev:preview": "vite preview dev",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . ",
    "prepublishOnly": "yarn lint && yarn build"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@stripe/stripe-js": "^2.1.2",
    "@types/jest": "^26.0.10",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/composition-api": "^1.0.0-rc.1",
    "eslint": "^8.48.0",
    "tsup": "^7.2.0",
    "typescript": "~5.1.6",
    "vite": "^4.4.9",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.8"
  }
}
