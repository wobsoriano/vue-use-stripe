{
  "name": "vue3-use-stripe",
  "version": "0.1.3",
  "description": "Thin Vue 3 wrapper for Stripe.js",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/vue-use-stripe#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/vue-use-stripe.git"
  },
  "bugs": {
    "url": "https://github.com/wobsoriano/vue-use-stripe/issues"
  },
  "keywords": [
    "stripe",
    "elements",
    "vue",
    "vue3",
    "typescript"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "build-fast": "tsup src/index.ts --format cjs,esm",
    "build": "pnpm build-fast --dts-resolve",
    "dev": "vite serve dev",
    "dev:build": "vite build dev",
    "dev:preview": "vite preview dev",
    "release": "bumpp && npm publish",
    "type-check": "vue-tsc --noEmit -p tsconfig.json --composite false",
    "lint": "eslint . ",
    "prepublishOnly": "pnpm lint && pnpm build"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "dependencies": {
    "@stripe/stripe-js": "^2.1.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "bumpp": "^9.2.0",
    "eslint": "^8.48.0",
    "tsup": "^7.2.0",
    "typescript": "~5.2.2",
    "vite": "^4.4.9",
    "vue": "^3.3.7",
    "vue-tsc": "^1.8.22"
  }
}
